{
  "name": "Paige",
  "description": "Your personal new tab companion. Plan, navigate, and personalize your space. Focus, inspire, and achieve with every new tab.",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "/logo/16x16.png",
    "48": "/logo/48x48.png",
    "64": "/logo/64x64.png",
    "128": "/logo/128x128.png"
  },
  "chrome_url_overrides": {
    "newtab": "./index.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": [
    "history",
    "bookmarks",
    "search",
    "favicon",
    "topSites",
    "notifications",
    "tabs"
  ],
  "web_accessible_resources": [
    {
      "resources": ["_favicon/*"],
      "matches": ["<all_urls>"],
      "extension_ids": ["*"]
    }
  ]
}
